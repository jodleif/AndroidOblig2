<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_vareliste"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jo.obligatorisk2.vare_listeFragment"
    tools:showIn="@layout/activity_vare_liste">


    <ListView
        android:id="@+id/vare_liste"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:layout_weight="1"
        android:background="#cac7c7"
        android:drawSelectorOnTop="true"
        android:listSelector="#6417ef13"
        android:padding="2dp"
        android:smoothScrollbar="true"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="0dp" />
</LinearLayout>
